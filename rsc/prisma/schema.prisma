// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Event {
  eventId     Int    @id @default(autoincrement())
  name        String
  date        String
  time        String
  location    String
  image       String
  contactInfo String
}
